<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Insider UI Test Suite" parallel="tests" thread-count="4">
    <listeners>
        <listener class-name="insider.listeners.ExtentTestNGITestListener"/>
    </listeners>
    <test name="Insider UI Tests">
        <parameter name="browser" value="${browser}"/>
        <classes>
            <class name="insider.tests.InsiderUiTest"/>
        </classes>
    </test>
</suite>